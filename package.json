{
    "name": "spdmainz",
    "version": "1.0.0",
    "description": "The WordPress Child Theme of SPD Mainz",
    "main": "index.js",
    "scripts": {
      "build": "sass ./spdmainz/scss/_main.scss ./spdmainz/css/style.css && sass ./spdmainz/scss/calendar-only.scss ./spdmainz/css/calendar-only.css && sass ./spdmainz/scss/team.scss ./spdmainz/css/team.css && sass ./spdmainz/admin/scss/admin.scss ./spdmainz/admin/css/admin.css",
      "watch": "sass --watch ./wp-content/themes/spdmainz/scss/_main.scss ./wp-content/themes/spdmainz/css/style.css && sass ./wp-content/plugins/spdmainz/scss/2click_youtube/style.scss ./wp-content/plugins/spdmainz/css/2click_youtube/style.css"
    },
    "author": "Erik Donner",
    "license": "ISC",
    "dependencies": {
      "sass": "^1.53.0"
    }
  }
  